    "gulp-path": "^3.0.3",
    "gulp-plumber": "^1.1.0",
    "gulp-rename": "^1.2.2",
    "mocha": "^2.4.5",
    "should": "^8.3.1"
  },
  "dependencies": {
    "commander": "^2.9.0",
    "request": "^2.72.0"
  },
  "repository": {
