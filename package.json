{
  "name": "gulp-blacklisted",
  "version": "1.0.0",
  "description": "check if your dependencies blacklisted.",
  "main": "index.js",
  "engines" : {
      "node" : ">=0.11 <0.12"
  },
  "bin": "./bin/gulp-blacklisted",
  "scripts": {
    "build": "./node_modules/gulp/bin/gulp.js coffee",
    "unit-test": "./node_modules/mocha/bin/mocha ./spec/spec.js",
