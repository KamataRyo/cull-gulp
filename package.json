{
  "name": "cull-gulp",
  "version": "1.0.0",
  "description": "check if your dependencies blacklisted.",
  "main": "CullGulp.js",
  "engines": {
    "node": ">=0.11 <0.12"
  },
  "bin": "./bin/cull-gulp",
  "scripts": {
    "build": "./node_modules/gulp/bin/gulp.js coffee",
    "unit-test": "./node_modules/mocha/bin/mocha ./spec/spec.js",
    "if-compiled": "ls ./bin/cull-gulp; ls ./BlackList.js; ls ./index.js",
    "cull-blackList": "./bin/cull-gulp --strict --quiet",
    "pretest": "set -e;npm run build; npm run cull-blackList",
    "test": "npm run if-compiled; npm run unit-test;"
  },
  "keywords": [
    "gulp",
    "plugins",
    "blacklist"
  ],
  "author": "KamataRyo",
  "license": "MIT",
  "devDependencies": {
    "coffee-script": "^1.10.0",
    "express": "^4.13.4",
    "gulp": "^3.9.1",
    "gulp-coffee": "^2.3.2",
    "mocha": "^2.4.5",
    "should": "^8.3.1"
  },
  "dependencies": {
    "commander": "^2.9.0",
    "request": "^2.72.0",
    "gulp-sketch": 1
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/KamataRyo/na-gulp.git"
  },
  "bugs": {
    "url": "https://github.com/KamataRyo/na-gulp/issues"
  },
  "homepage": "https://github.com/KamataRyo/na-gulp#readme",
  "blackListURL": "http://gulpjs.com/plugins/blackList.json"
}
