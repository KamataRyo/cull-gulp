{
  "name": "cull-gulp",
  "version": "1.1.4",
  "description": "check if your dependencies blacklisted.",
  "main": "CullGulp.js",
  "engines": {
    "node": ">=0.11 <0.12"
  },
  "bin": "./bin/cull-gulp",
  "scripts": {
    "build": "./node_modules/gulp/bin/gulp.js coffee",
    "if-compiled": "ls ./CullGulp.js; ls ./bin/bin.js",
    "unit-test": "./node_modules/mocha/bin/mocha ./spec/spec.js",
    "cull": "./bin/cull-gulp --strict --quiet",
    "test": "npm run if-compiled; npm run unit-test;",
    "prepublish": "npm run build"
  },
  "keywords": [
    "gulp",
    "plugins",
    "blacklist"
  ],
  "author": "KamataRyo",
  "license": "MIT",
  "devDependencies": {
    "coffee-script": "^1.10.0",
    "express": "^4.13.4",
    "gulp": "^3.9.1",
    "gulp-coffee": "^2.3.2",
    "mocha": "^2.4.5",
    "should": "^8.3.1"
  },
  "dependencies": {
    "cli-color": "^1.1.0",
    "commander": "^2.9.0",
    "request": "^2.72.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/KamataRyo/na-gulp.git"
  },
  "bugs": {
    "url": "https://github.com/KamataRyo/na-gulp/issues"
  },
  "homepage": "https://github.com/KamataRyo/na-gulp#readme",
  "config": {
    "blackListURL": "http://gulpjs.com/plugins/blackList.json"
  }
}
